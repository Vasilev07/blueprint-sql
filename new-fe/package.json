        "@typescript-eslint/parser": "^8.54.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "jest": "^30.0.0",
        "jest-environment-jsdom": "^30.0.0",
        "jest-preset-angular": "^16.0.0",
        "typescript": "~5.9.3",
