{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/",
        "coverage/"
    ],
    "watch": [
        "src"
    ],
    "verbose": true,
    "execMap": {
        "ts": "ts-node"
    },
  "exec": "ts-node ./src/index.ts",
  "env": {
        "NODE_ENV": "development",
        "JWT_KEY": "secred"
    },
    "ext": "js,json,ts"
}